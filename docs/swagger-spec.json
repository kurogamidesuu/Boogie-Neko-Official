  "info": {
    "title": "Boogie Neko API",
    "description": "The official API for the Boogie Neko store",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@boogieneko.com",
            "description": "The email of the user"
          },
          "password": {
            "type": "string",
            "example": "StrongPassword123",
            "minLength": 6
          },
          "firstName": {
            "type": "string",
            "example": "Teru"
          },
          "lastName": {
            "type": "string",
            "example": "Neko"
          },
          "role": {
            "type": "string",
            "enum": [
              "USER",
              "ADMIN"
            ],
            "default": "USER"
          }
        },
        "required": [
          "email",
          "password",
          "firstName",
          "lastName"
        ]
      },
      "UpdateUserDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@boogieneko.com",
            "description": "The email of the user"
          },
          "password": {
            "type": "string",
            "example": "StrongPassword123",
            "minLength": 6
          },
          "firstName": {
            "type": "string",
            "example": "Teru"
          },
          "lastName": {
            "type": "string",
            "example": "Neko"
          },
          "role": {
            "type": "string",
            "enum": [
              "USER",
              "ADMIN"
            ],
            "default": "USER"
          }
        }
      },
      "CreateProductDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "example": "Pearl Necklace"
          },
          "description": {
            "type": "string",
            "example": "A beautiful handmade pearl necklace."
          },
          "price": {
            "type": "number",
            "example": "49.50"
          },
          "stock": {
            "type": "number",
            "example": "10",
            "minimum": 0
          },
          "categoryId": {
            "type": "number",
            "example": 2
          }
        },
        "required": [
          "title",
          "description",
          "price",
          "stock",
          "categoryId"
        ]
      },
      "UpdateProductDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "example": "Pearl Necklace"
          },
          "description": {
            "type": "string",
            "example": "A beautiful handmade pearl necklace."
          },
          "price": {
            "type": "number",
            "example": "49.50"
          },
          "stock": {
            "type": "number",
            "example": "10",
            "minimum": 0
          },
          "categoryId": {
            "type": "number",
            "example": 2
          }
        }
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@boogieneko.com",
            "description": "The email of the user"
          },
          "password": {
            "type": "string",
            "example": "StrongPassword123",
            "minLength": 6
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "AddToCartDto": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "number",
            "example": 3
          },
          "quantity": {
            "type": "number",
            "example": 3,
            "minimum": 1
          }
        },
        "required": [
          "productId",
          "quantity"
        ]
      },
      "CreateOrderDto": {
        "type": "object",
        "properties": {
          "shippingAddress": {
            "type": "string",
            "example": "Nya Nya Apartment, Neko Land, Tokyo, Japan",
            "description": "The address where the products are shipped to"
          }
        },
        "required": [
          "shippingAddress"
        ]
      }
    }
  }
